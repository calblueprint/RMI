<!DOCTYPE html>
<html>
  <head>
    <title>Rmi</title>
    <%= csrf_meta_tags %>

    <%= javascript_include_tag('application.js') %>
    <%= javascript_pack_tag "application" %>
    <%= stylesheet_link_tag 'application', media: 'all'  %>
    <script>
      window.INITIAL_STATE = <%= raw @state.to_json %>
    </script>
  </head>

  <body>
    <p class="notice"><%= notice %></p>
    <p class="alert"><%= alert %></p>
    <%= yield %>
  </body>
</html>
